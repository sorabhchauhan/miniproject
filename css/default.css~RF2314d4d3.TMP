/*    HEADER AND SHEET DIVISION   */

#header_top {
	width: 100%;
	height: 46px;
	background: #000;

	//background: url(../images/header_top.png)repeat-x; /*The Image has a lot of contrast and doesn't fit with the surrounding soft coloring */

-webkit-box-shadow: 0px 2px 10px black;
	box-shadow: 0px 2px 10px black;
	position: absolute;
	z-index: 5;
	z-index: 1;
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
	filter: alpha(opacity=100);
	opacity: 1;
	
	float:right;
	margin-right:30px;
}

#sheet {
	z-index: 0;
	width: 900px;
	margin: auto;
	margin-top: 0px;
	padding-top: 0px;
	overflow: hidden;

	-webkit-box-shadow: 0 0 10px rgba(0,0,0,.4);
	box-shadow: 0 0 10px rgba(0,0,0,.4);
}

#sheet #header {
	width: 900px;
	height: 220px;
	//background: #3399FF;
	background:url(../images/header_built_1.jpg)no-repeat;
	
	

	-webkit-box-shadow: 0 0 20px #333333;
	box-shadow: 0 0 20px #333333;
	position: relative;
	margin-top: 30px;
	border-left: 1px solid #33CCFF;
	border-right: 1px solid #33CCFF;

	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
}


/*      FAKE LOGO PART   */
#sheet #header img{
	width:210px;
	height:188px;
	margin-left:14px;
	margin-top:32px;
	float:left;
	opacity:0;                          /*  <!-- 1 means True and 0 means False --> */
	}

#header_bg {
	//z-index: -6;
	height: 220px;
	width: 100%;
	background: url(../images/header_base.JPG);
	opacity:.7;
	margin-top: 30px;
	position: absolute;
	border-bottom: 1px solid #00FFFF;
}

#sheet #menu {
	z-index: -1;
	background: #F90;
	height: 50px;
	width: 100%;

	-webkit-box-shadow: 0 2px 10px #000000;
	box-shadow: 0 2px 10px #000000;
	position: relative;
}

/*   WEBPAGE MAIN BODY  */
#sheet #main_body {
	z-index: -2;
	padding-top: 40px;
	padding-left: 5px;
	padding-right: 0px;
	background: #FFF;
}

/*    WEBSITE GUTTER-BOX   */
#sheet #gutterBOX {
	height: 120px;
	width: 100%;
	position: relative;

	-webkit-box-shadow: 0 -2px 10px black;
	box-shadow: 0 -2px 10px black;
	background: #09F;
	z-index: 1;
}

#gutterBOX_bg {
	position: absolute;
	width: 100%;
	height: 120px;
	background: #000;
	overflow: hidden;
	z-index: 0;
	margin-top: -120px;
	border-top:1px solid #666;
}

/*   MAIN TAGS    */
p {
	padding-bottom: 20px;
}

body {
	background: url(../images/body_color.jpg)repeat-x;
}

h1,h2,h3,h4,h5 {
	margin: 25px 0;
}

h3
{
	font-size:18px;
	font-weight:700 !important;	
}

#sheet #main_body #widgets {
	float: left;
	padding: 5px;
	padding-top: 10px;
	margin-top: -30px;
	margin-right: 10px;

	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	background: #99FF00;
	width: 210px;
	height: 600px;

	-webkit-box-shadow: 0 0 8px rgba(0,0,0,.6);
	box-shadow: 0 0 8px rgba(0,0,0,.6);
}

/* UPDATE TO CSS3 BY TANMAY SAXENA VERSION 1.01 */

#sheet #main_body #main_data p {
	font-family: Georgia, "Times New Roman", Times, serif;
	font-size: 12px;
	line-height: 20px;
}

#sheet #main_body #right_widgets {
	padding: 5px;
	background:#FC0;
	-webkit-box-shadow: 0 0 8px rgba(0,0,0,.6);
	box-shadow: 0 0 8px rgba(0,0,0,.6);
	float: right;
	width: 150px;
	height: 600px;
	padding-right: 10px;
	margin-right: 0;
	margin-top: -900px;
	margin-left: 0;
}

#sheet #main_body table {
	margin-top: 200px;
	padding: 10px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	
}

/* BASIC DEFAULT STYLES */
p#quote {
	font-family: Georgia, "Times New Roman", Times, serif;
	font-style: italic;
	font-size: 18px;
	color: #FF6600;
	font-weight: bolder;
}

p#author {
	text-align: right;
	padding-top: 0;
	font-family: Georgia, "Times New Roman", Times, serif;
	font-style: italic;
	font-size: 18px;
	color: #0000FF;
	font-weight: bold;
}

p#adj {
	text-align: center;
	font-size:inherit !important;
	}

#sheet #gutterBOX p {
	text-align: center;
	padding: 0;
	font-size: 14px;
	color: #FFF;
	line-height: 18px;
}

#sheet #gutterBOX p#copyrights {
	padding-top: 20px;
}

#sheet #gutterBOX p#designer {
	line-height: 18px;
}

#sheet #gutterBOX p#copyrights a,#sheet #gutterBOX p#designer a {
	text-decoration: none;
	padding: 0;
	font-size: 14px;
	color: #FFF;
	line-height: 20px;
}


/* UPDATE TO CSS3 BY TANMAY SAXENA - UPDATE 1.02   */

/*  PARAGRAPH FIXES - MAIN BODY, LINKS AND OTHERS  */


#header_top #menu_top ul{
	  	float:right;
	}
	
p#important {
	font-family:"Comic Sans MS", cursive;
	font-size:15px;
	font-weight:300;
	text-align:justify;
	}
	
p#right_widgets_section {
	color:#000;
	font-family:Verdana, Geneva, sans-serif;
	font-size:14px;
	text-align:left;
	margin-top:50px;
	}
	
a#red
{
	color:#000 !important;
	font-weight:bold;
	}

#sheet #header ul#links
{
	margin-top:10px;
	float:right;
	}

#links li{
	text-align:center;
	background:#FF0;
	width:70px;
	height:60px;
	top:50px;
	float:left;
	display:block;
	padding-left:20px;
	padding-right:20px;
	padding-bottom:10px;
	padding-top:50px;
	margin-right:5px;
	box-shadow:3px -4px 5px  #000000;
	}

#links li:hover{
	background:#0F0;
	box-shadow:0 0 20px  #000000;
	}
	
#links li#fb:hover{
	background:#03C;
	}
	
#links li a
{
	list-style-type:none;
	text-transform:uppercase;
	}
